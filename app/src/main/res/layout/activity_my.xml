        <!--<TextView-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="@dimen/qb_px_45"-->
            <!--android:layout_marginLeft="@dimen/qb_px_15"-->
            <!--android:gravity="center"-->
            <!--android:text="注销账号"-->
            <!--android:textColor="#43496a"-->
            <!--android:textSize="15sp" />-->

        <!--<ImageView-->
            <!--android:layout_width="@dimen/qb_px_45"-->
            <!--android:layout_height="@dimen/qb_px_45"-->
            <!--android:layout_alignParentRight="true"-->
            <!--android:padding="@dimen/qb_px_15"-->
            <!--android:src="@mipmap/ic_click" />-->


    <!--</RelativeLayout>-->


    <Button
        android:id="@+id/btnLoginOut"
        android:layout_width="match_parent"
        android:layout_height="@dimen/qb_px_45"
        android:layout_marginLeft="@dimen/qb_px_60"
        android:layout_marginTop="@dimen/qb_px_80"
        android:layout_marginRight="@dimen/qb_px_60"
        android:background="@drawable/shape_login_out"
        android:text="退出登录"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cache" />

</androidx.constraintlayout.widget.ConstraintLayout>